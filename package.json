{
  "name": "@brillout/path-to-regexp",
  "main": "./dist/matchPath.js",
  "version": "0.2.1",
  "description": "Vanilla copy of React Router's path-to-regexp mod",
  "scripts": {
    "dev": "npm run tsc:watch",
    "build": "npm run tsc:once",
    "tsc:watch": "tsc --build --watch",
    "tsc:once": "tsc --build",
    "clean": "git clean -Xdf",
    "prepublishOnly": "npm run clean && npm install && npm run build"
  },
  "dependencies": {
    "path-to-regexp": "^1.7.0"
  },
  "devDependencies": {
    "@types/node": "^14.11.2",
    "typescript": "^4.0.3"
  },
  "repository": "github:brillout/path-to-regexp",
  "publishConfig": {
    "access": "public"
  }
}
